<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1.5dp"
        android:top="1.5dp">
        <shape android:shape="rectangle">
            <corners android:radius="50dp"/>
            <solid android:color="@color/black_333333"/>
            <size
                android:width="50dp"
                android:height="100dp"/>
            <gradient
                android:angle="270"
                android:endColor="@color/black_333333"
                android:startColor="@color/gray_888888"/>
            <stroke android:color="@color/black_333333"/>
        </shape>
    </item>
    <item
        android:bottom="1.5dp"
        android:right="1.5dp">
        <shape android:shape="rectangle">
            <corners android:radius="50dp"/>
            <solid android:color="@color/black_333333"/>
            <size
                android:width="50dp"
                android:height="100dp"/>
            <gradient
                android:angle="270"
                android:endColor="@color/black_333333"
                android:startColor="@color/gray_888888"/>
            <stroke android:color="@color/black_333333"/>
        </shape>
    </item>

</layer-list>